  "types": "lib/index.d.ts",
  "keywords": [
    "koishi",
    "koishi-plugin"
  ],
  "peerDependencies": {
    "koishi": "^4.18.9"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.4.0"
  },
  "scripts": {
    "build": "tsc -p .",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "author": "sdxcvbot <zhufuzhi1@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdxcvbot/koishi-plugin-default-auth-zero.git"
  },
  "bugs": {
    "url": "https://github.com/sdxcvbot/koishi-plugin-default-auth-zero/issues"
  },
  "homepage": "https://github.com/sdxcvbot/koishi-plugin-default-auth-zero#readme",
  "files": [
    "lib",
    "src",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "koishi": {
    "description": {
      "zh": "仅将新绑定/未设权限的 OneBot(QQ) 用户设置为 authority=0；已绑定或手动授权的用户不修改。"
